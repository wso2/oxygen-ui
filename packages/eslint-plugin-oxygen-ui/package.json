{
  "name": "@wso2/eslint-plugin-oxygen-ui",
  "version": "0.0.1-alpha.0",
  "description": "ESLint plugin to prevent @mui and lucide-react direct imports in Oxygen UI based projects.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "eslint",
    "plugin",
    "oxygen-ui",
    "material-ui",
    "lucide-react",
    "imports"
  ],
  "author": "WSO2 LLC",
  "license": "Apache-2.0",
  "peerDependencies": {
    "eslint": "^8.0.0 || ^9.0.0"
  },
  "devDependencies": {
    "@types/eslint": "catalog:",
    "@types/estree": "^1.0.6",
    "esbuild": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  },
  "scripts": {
    "build": "rm -rf dist && node esbuild.config.js && tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": []
      }
    }
  }
}

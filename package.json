{
  "private": true,
  "name": "@wso2/oxygen-ui-workspace",
  "description": "WSO2 Oxygen UI | Workspace",
  "files": [
    "packages",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "nx run-many --target=build --all",
    "build:sample": "cd samples/oxygen-ui-test-app && BUILD_FOR_STORYBOOK=true pnpm build",
    "build:storybook": "pnpm build:sample && pnpm --filter @wso2/oxygen-ui-docs build",
    "clean": "nx run-many --target=clean --all",
    "typecheck": "nx run-many --target=typecheck --all",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint:fix --all",
    "prepublishOnly": "nx run-many --target=build --all",
    "publish:packages": "changeset publish",
    "version:packages": "changeset version && pnpm install --lockfile-only",
    "test": "nx run-many --target=test --all",
    "storybook": "pnpm --filter @wso2/oxygen-ui-docs dev"
  },
  "keywords": [
    "wso2",
    "oxygen-ui",
    "design-system",
    "material-ui",
    "mui",
    "react",
    "typescript",
    "components",
    "ui-library"
  ],
  "author": "WSO2 LLC",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wso2/oxygen-ui.git"
  },
  "bugs": {
    "url": "https://github.com/wso2/oxygen-ui/issues"
  },
  "homepage": "https://github.com/wso2/oxygen-ui#readme",
  "devDependencies": {
    "@changesets/changelog-github": "0.5.1",
    "@changesets/cli": "2.29.7",
    "fs-extra": "11.3.2",
    "nx": "22.0.2",
    "react-is": "19.0.0"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "pnpm": {
    "overrides": {
      "react-is": "19.0.0"
    }
  },
  "publishConfig": {
    "access": "restricted"
  }
}

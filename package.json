    "@storybook/react-webpack5": "^10.0.2",
    "babel-loader": "^10.0.0",
    "nx": "22.0.1",
    "rimraf": "^6.0.1",
    "storybook": "^10.0.2",
    "vite": "^7.1.12",
    "vitest": "^1.0.4"
